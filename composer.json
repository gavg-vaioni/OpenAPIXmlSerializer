{
    "name": "vaioni/open-api-xml-serializer",
    "type": "library",
    "description": "PHP Xml Serializer, with annotation support.",
    "keywords": ["xml","serialization","annotations"],
    "homepage": "https://github.com/gavg-vaion/OpenApiXmlSerializer",
    "license": "CC BY-NA",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "vaioni/city_fibre_sdk": "0.0.1",
        "phpunit/phpunit": "^11.2",
        "squizlabs/php_codesniffer": "^3.10",
        "infection/infection": "^0.29.6",
        "doctrine/coding-standard": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Vaioni\\OpenApiXmlSerializer\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://bitbucket.org/vaioni1/cityfibresdk.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
